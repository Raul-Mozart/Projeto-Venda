<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Venda"
                   namespace="Venda.Entidades">

	<class name="Cliente" table="clientes">
		<id name="Codigo" type="int">
			<generator class="sequence" >
				<param name="sequence">cliente_seq</param>
			</generator>
		</id>
		<property	name="Nome"				type="string"	length="50"		/>
		<property	name="CPF"				type="string"	length="50"		/>
		<property	name="DataNascimento"	type="Date"			/>
		<property	name="Email"			type="string"	length="50"		/>

	</class>

</hibernate-mapping>